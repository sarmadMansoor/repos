<h1>Details of All user in dataBase</h1>
@model List<lab_web_05.Models.User>
@if(Model.Count>0)
{
    @foreach(var user in Model)
    {
        <h3>User Id is : @user.Id</h3>
        <h3>User Name is : @user.Name</h3>
        <h3>User Email is : @user.Email</h3>
        <h3>User Password is : @user.Password</h3>
        <h3>User Creation data  is : @user.CreatedDate.ToString("dd MMMM yyyy hh:mm:ss tt")</h3>
        <br />
        <br />
        <br />
    }
}
else
{
    <h3>There is no User in the DataBase</h3>
}